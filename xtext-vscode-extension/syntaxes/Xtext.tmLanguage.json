{
    "fileTypes": [
        "xtext"
    ],
    "name": "Xtext",
    "patterns": [
        {
            "include": "#main"
        }
    ],
    "scopeName": "source.Xtext",
    "uuid": "",
    "repository": {
        "main": {
            "patterns": [
                {
                    "include": "#grammar"
                }
            ]
        },
        "Action": {
            "patterns": [
                {
                    "match": "({)((?:\\w+::)?\\^?\\w+\\.?\\w*)(\\s*)(\\+?=?)(\\s*)((?:current)?)(})",
                    "captures": {
                        "1": {
                            "name": "punctuation.Xtext"
                        },
                        "2": {
                            "name": "entity.name.Xtext"
                        },
                        "3": {
                            "name": "whitespace.Xtext"
                        },
                        "4": {
                            "name": "keyword.operator.Xtext"
                        },
                        "5": {
                            "name": "whitespace.Xtext"
                        },
                        "6": {
                            "name": "keyword.control.Xtext"
                        },
                        "7": {
                            "name": "punctuation.Xtext"
                        }
                    }
                }
            ]
        },
        "Annotation": {
            "patterns": [
                {
                    "match": "(@\\w+)",
                    "name": "support.type.Xtext"
                }
            ]
        },
        "FeatureAssignment": {
            "patterns": [
                {
                    "match": "(\\^?\\w+)(\\s*)((?:[\\+\\?]=)|(?:=))",
                    "captures": {
                        "1": {
                            "name": "entity.name.Xtext"
                        },
                        "2": {
                            "name": "whitespace.Xtext"
                        },
                        "3": {
                            "name": "keyword.operator.Xtext"
                        }
                    }
                }
            ]
        },
        "Keywords": {
            "patterns": [
                {
                    "match": "(\\bgrammar\\b)|(\\bwith\\b)|(\\bhidden\\b)|(\\bgenerate\\b)|(\\bas\\b)|(\\bimport\\b)|(\\bfragment\\b)|(\\breturns\\b)|(\\btrue\\b)(\\bfalse\\b)|(\\bterminal\\b)|(\\bfragment\\b)|(\\breturns\\b)|(\\bEOF\\b)|(\\benum\\b)|(\\beturns\\b)",
                    "name": "keyword.control.Xtext"
                }
            ]
        },
        "Operators": {
            "patterns": [
                {
                    "match": "(\\*|&|\\?|\\*|\\+|\\||&|!|(=>)|(\\.\\.)|(\\->)|\\[|\\])",
                    "name": "keyword.operator.Xtext"
                }
            ]
        },
        "Punctuation": {
            "patterns": [
                {
                    "match": "(,|\\,|:|;|<|>|\\(|\\))",
                    "name": "punctuation.Xtext"
                }
            ]
        },
        "STRING": {
            "patterns": [
                {
                    "begin": "(\\\")",
                    "beginCaptures": {
                        "1": {
                            "name": "string.quoted.double.Xtext"
                        }
                    },
                    "contentName": "string.quoted.double.Xtext",
                    "end": "(\\\")",
                    "endCaptures": {
                        "1": {
                            "name": "string.quoted.double.Xtext"
                        }
                    }
                },
                {
                    "begin": "(\\')",
                    "beginCaptures": {
                        "1": {
                            "name": "string.quoted.single.Xtext"
                        }
                    },
                    "contentName": "string.quoted.single.Xtext",
                    "end": "(\\')",
                    "endCaptures": {
                        "1": {
                            "name": "string.quoted.single.Xtext"
                        }
                    }
                }
            ]
        },
        "STRING__1": {
            "patterns": []
        },
        "STRING__2": {
            "patterns": []
        },
        "escapedName": {
            "patterns": [
                {
                    "match": "(\\^\\w+)",
                    "name": "entity.name.Xtext"
                }
            ]
        },
        "fullQualifiedName": {
            "patterns": [
                {
                    "match": "(\\^?(?:\\w+::)?\\b\\w+(\\.|\\$\\w+)*\\b)",
                    "name": "entity.name.Xtext"
                }
            ]
        },
        "grammar": {
            "patterns": [
                {
                    "include": "#multi_line_comment"
                },
                {
                    "include": "#single_line_comment"
                },
                {
                    "include": "#Keywords"
                },
                {
                    "include": "#Operators"
                },
                {
                    "include": "#Punctuation"
                },
                {
                    "include": "#Action"
                },
                {
                    "include": "#FeatureAssignment"
                },
                {
                    "include": "#fullQualifiedName"
                },
                {
                    "include": "#escapedName"
                },
                {
                    "include": "#Annotation"
                },
                {
                    "include": "#STRING"
                },
                {
                    "match": "([^\\s])",
                    "name": "invalid.Xtext"
                }
            ]
        },
        "multi_line_comment": {
            "patterns": [
                {
                    "begin": "(\/\\*)",
                    "beginCaptures": {
                        "1": {
                            "name": "comment.block.Xtext"
                        }
                    },
                    "contentName": "comment.block.Xtext",
                    "end": "(\\*\/)",
                    "endCaptures": {
                        "1": {
                            "name": "comment.block.Xtext"
                        }
                    }
                }
            ]
        },
        "multi_line_comment__1": {
            "patterns": []
        },
        "single_line_comment": {
            "patterns": [
                {
                    "match": "(\/\/.*)",
                    "name": "comment.line.double-slash.Xtext"
                }
            ]
        }
    }
}