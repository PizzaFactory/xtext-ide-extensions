{
    "name": "xtend-vscode-extension",
    "displayName": "xtend-vscode-extension",
    "description": "Syntax Highlighting for the Xtend Programming Language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "xtend",
            "aliases": ["Xtend", "xtend"],
            "extensions": [".xtend"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "xtend",
            "scopeName": "source.Xtend",
            "path": "./syntaxes/xtend.tmLanguage.json"
        }]
    }
}